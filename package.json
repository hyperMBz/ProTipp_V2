  "private": true,
  "scripts": {
    "dev": "next dev -H 0.0.0.0",
    "build": "next build",
    "start": "next start",
    "lint": "bunx tsc --noEmit && next lint",
    "format": "bunx biome format --write",
    "test": "vitest",
    "test:run": "vitest run",
    "test:unit": "vitest run src/components/__tests__/",
    "test:integration": "vitest run src/tests/integration/",
    "test:components": "vitest run src/components/__tests__/",
    "test:analytics": "vitest run src/components/analytics/__tests__/",
    "test:bet-tracker": "vitest run src/components/bet-tracker/__tests__/",
    "test:calculator": "vitest run src/components/calculator/__tests__/",
    "test:all": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest run --coverage --ui",
    "test:watch": "vitest --watch",
    "test:performance": "k6 run src/lib/testing/performance-tests/",
    "test:security": "npm audit --audit-level=moderate",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:ci": "bun run test:all && bun run test:coverage && bun run test:e2e",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate-vapid": "node scripts/generate-vapid-keys.js",
    "test:email": "node scripts/test-email-config.js",
    "test:sms": "node scripts/test-sms-config.js",
    "deploy:production": "bash scripts/deploy-production.sh",
    "performance:monitor": "node scripts/performance-monitor.js",
    "performance:analyze": "ANALYZE=true bun run build",
    "lighthouse:audit": "node scripts/lighthouse-optimization.js",
    "lighthouse:optimize": "bun run dev & sleep 5 && node scripts/lighthouse-optimization.js http://localhost:3000",
    "netlify:build": "bun run build && node scripts/netlify-build-optimization.js",
    "netlify:deploy": "bun run netlify:build && echo 'Ready for Netlify deployment'"
  },
